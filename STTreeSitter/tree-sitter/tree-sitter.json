{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "sample",
      "camelcase": "Sample",
      "title": "Sample",
      "scope": "source.sample",
      "file-types": [
        "sample"
      ],
      "injection-regex": "^sample$",
      "class-name": "TreeSitterSample"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Sample grammar for tree-sitter",
    "authors": [
      {
        "name": "MPW"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-sample"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}